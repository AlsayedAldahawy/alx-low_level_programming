#include <stdio.h>

/**
 * main - prints the first 50 Fibonacci numbers
 * Return: 0 if success
 */

int main(void)
{
	long unsigned Fn, Fn_lft, Fn_rgt, F1_lft = 0, F2_lft = 0, F1_rgt = 1, F2_rgt = 2;
	int i, n = 1;

	for ( i = 0; i < 98; i++)
	{	
		Fn_lft = F1_lft + F2_lft;
		Fn_rgt = F1_rgt + F2_rgt;

		if (Fn_rgt > 9)
		{
			Fn_lft ++;
			Fn_rgt = Fn_rgt / 10;
		}
		
		F1_lft = F2_lft;
		F1_rgt = F2_rgt;

		F2_lft = Fn_lft;
		F2_rgt = Fn_rgt;

		if (Fn_lft = 0)
		{
			printf("%lu", Fn_rgt);
		}
		else
		{
			printf("%lu%lu", Fn_lft,Fn_rgt);
		}
	}
	return (0);
}
